        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// ----------------PARTE 1: VALIDAÇÕES-------------------


// Normalizar nome da cidade (remover acentos, uppercase)
var nome_cidade_normalizado = nome_cidade
    .replace(/[áàâãÁÀÂÃ]/g, "A")
    .replace(/[éèêÉÈÊ]/g, "E")
    .replace(/[íìÍÌ]/g, "I")
    .replace(/[óòôõÓÒÔÕ]/g, "O")
    .replace(/[úùÚÙ]/g, "U")
    .replace(/ç/g, "c")
    .replace(/Ç/g, "C")
    .toUpperCase();

// Validar temperaturas (entre -50 e 60)
var temp_valida = (temp_min >= -50 &amp;&amp; temp_min &lt;= 60 &amp;&amp; 
                   temp_max >= -50 &amp;&amp; temp_max &lt;= 60 &amp;&amp; temp_min &lt;= temp_media &amp;&amp; temp_media &lt;= temp_max);

// Validar precipitação (0-100%)
if (precipitacao_prob &lt; 0 || precipitacao_prob > 100) {
    precipitacao_prob = 0.0;
}

// Validar direção do vento
var direcoes_validas = /^(N|S|E|W|NE|NW|SE|SW|NNE|NNW|SSE|SSW|ENE|ESE|WNW|WSW)$/i;
if (!direcoes_validas.test(vento_direcao)) {
    vento_direcao = "N/A";
}

// Status de validação geral
var validacao_status = (data_valida &amp;&amp; temp_valida) ? "VALIDO" : "INVALIDO";</jsScript_script>
