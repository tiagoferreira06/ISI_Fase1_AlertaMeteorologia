      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>
// -------------CALCULOS------------
// Calcular temperatura média
var temp_media = (temp_min + temp_max) / 2;

// Converter para Fahrenheit
var temp_media_fahrenheit = temp_media * 9/5 + 32;

// Converter para Kelvin
var temp_media_kelvin = temp_media + 273.15;

// Classificação de temperatura
var classificacao_temp = "";
if (temp_media &lt; 10) {
    classificacao_temp = "Frio";
} else if (temp_media &lt; 18) {
    classificacao_temp = "Fresco";
} else if (temp_media &lt; 25) {
    classificacao_temp = "Agradável";
} else if (temp_media &lt; 30) {
    classificacao_temp = "Quente";
} else {
    classificacao_temp = "Muito Quente";
}
</jsScript_script>
