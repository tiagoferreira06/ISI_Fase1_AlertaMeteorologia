      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// ========== ANÁLISE DA PREVISÃO ==========
var nivel_alerta = "";
var tipo_alerta = "";
var mensagem = "";
var acao_recomendada = "";

// ALERTA 1: Temperatura Extrema
if (temp_media > 35) {
    nivel_alerta = "CRITICO";
    tipo_alerta = "Calor Extremo";
    mensagem = "Temperatura muito elevada: " + temp_media + "°C";
    acao_recomendada = "Risco de ondas de calor";
    
} else if (temp_media > 30) {
    nivel_alerta = "ALTO";
    tipo_alerta = "Calor Intenso";
    mensagem = "Temperatura elevada: " + temp_media + "°C";
    acao_recomendada = "Aumentar hidratacao, evitar exposicao solar";
    
} else if (temp_media &lt; 5) {
    nivel_alerta = "CRITICO";
    tipo_alerta = "Frio Extremo";
    mensagem = "Temperatura muito baixa: " + temp_media + "°C";
    acao_recomendada = "Ficar em Ambientes Quentes";
    
} else if (temp_media &lt; 10) {
    nivel_alerta = "MEDIO";
    tipo_alerta = "Frio Intenso";
    mensagem = "Temperatura baixa: " + temp_media + "°C";
    acao_recomendada = "Agasalhar-se adequadamente";
    
} else {
    nivel_alerta = "NORMAL";
    tipo_alerta = "Temperatura Confortavel";
    mensagem = "Temperatura dentro dos parametros normais";
}

// ALERTA 2: Chuva
if (precipitacao_prob > 80) {
    nivel_alerta = nivel_alerta == "CRITICO" ? "CRITICO" : "ALTO";
    tipo_alerta = tipo_alerta + " + Chuva Muito Provavel";
    mensagem = mensagem + ". Probabilidade de chuva: " + precipitacao_prob + "%";
    acao_recomendada = acao_recomendada + ". Chuva quase certa - levar guarda-chuva";
} else if (precipitacao_prob > 50) {
    if (nivel_alerta == "NORMAL") nivel_alerta = "MEDIO";
    tipo_alerta = tipo_alerta + " + Chuva Provavel";
    mensagem = mensagem + ". Probabilidade de chuva: " + precipitacao_prob + "%";
    acao_recomendada = acao_recomendada + ". levar guarda-chuva";
} else if (precipitacao_prob > 20) {
    tipo_alerta = tipo_alerta + " + Possibilidade de Chuva";
    mensagem = mensagem + ". Probabilidade de chuva: " + precipitacao_prob + "%";
    acao_recomendada = acao_recomendada + ". Considerar levar guarda-chuva";
}

// ALERTA 3: Amplitude Termica
var amplitude = temp_max - temp_min;
if (amplitude > 15) {
    mensagem = mensagem + ". Grande variacao termica: " + amplitude.toFixed(1) + "°C";
    acao_recomendada = acao_recomendada + ". Preparar roupa para varias temperaturas";
}

if (acao_recomendada == "") {
 acao_recomendada = "Nenhuma acao necessaria";
}

// ========== CLASSIFICAÇÕES ==========

var risco_saude = "";
if (temp_media > 32 || temp_media &lt; 5) risco_saude = "ALTO";
else if (temp_media > 28 || temp_media &lt; 10) risco_saude = "MEDIO";
else risco_saude = "BAIXO";

var regiao = "";
if (nome_cidade_normalizado == "PORTO" || nome_cidade_normalizado == "BRAGA" || nome_cidade_normalizado == "AVEIRO") regiao = "Norte";
else if (nome_cidade_normalizado == "LISBOA" || nome_cidade_normalizado == "COIMBRA" || nome_cidade_normalizado == "LEIRIA" || nome_cidade_normalizado == "SETUBAL") regiao = "Centro";
else if (nome_cidade_normalizado == "FARO" || nome_cidade_normalizado == "EVORA" || nome_cidade_normalizado == "BEJA") regiao = "Sul";
else regiao = "Outro";

// ========== PONTUAÇAO PARA RISCO==========
var score_risco = 0;

// Temperatura extrema
if (temp_media > 35) score_risco += 40;
else if (temp_media > 32) score_risco += 25;
else if (temp_media > 30) score_risco += 15;
else if (temp_media &lt; 5) score_risco += 40;
else if (temp_media &lt; 10) score_risco += 25;

// Chuva - NAO FUNCIONA POIS API NAO ESTA  A ENVIAR INTENSIDADE DA CHUVA
if (precipitacao_prob > 80) score_risco += 30;
else if (precipitacao_prob > 50) score_risco += 15;

// Amplitude termica
if (amplitude > 20) score_risco += 15;
else if (amplitude > 15) score_risco += 25;

// Vento 
if (descricao_tempo == "Forte") score_risco += 15;

var prioridade = "";
if (score_risco >= 70) prioridade = "URGENTE";
else if (score_risco >= 40) prioridade = "ALTA";
else if (score_risco >= 15) prioridade = "MEDIA";
else prioridade = "BAIXA";

var timestamp = new Date().toISOString();

// Criar flags para cada tipo de alerta
var flag_critico = (nivel_alerta == "CRITICO") ? 1 : 0;
var flag_alto = (nivel_alerta == "ALTO") ? 1 : 0;
var flag_medio = (nivel_alerta == "MEDIO") ? 1 : 0;
var flag_normal = (nivel_alerta == "NORMAL") ? 1 : 0;

</jsScript_script>
